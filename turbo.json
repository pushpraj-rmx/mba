{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "globalDependencies": [
    "**/.env*"
  ],
  "globalEnv": [
    "NODE_ENV",
    "LOG_LEVEL"
  ],
  "concurrency": "20",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "env": [
        "NODE_ENV",
        "LOG_LEVEL"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**",
        "web-build/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "env": [
        "NODE_ENV",
        "LOG_LEVEL"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true
    }
  }
}